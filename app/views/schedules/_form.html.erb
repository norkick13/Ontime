<%= form_for @schedule do |f| %>
<% if @schedule.errors.any? %>
<div class="error">
	<h2><%= Pluralize(@schedule.errors.count, "error") %> prevented this schedule from saving</h2>
	<ul>
		<% @schedule.errors_full_messages.each do |msg| %>
		<li><%= msg %></li>
		<% end %>
	</ul>
</div>
<% end %>

